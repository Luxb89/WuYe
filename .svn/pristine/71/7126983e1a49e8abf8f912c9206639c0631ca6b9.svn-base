function StrUtil(){
	
}
StrUtil.Strnull = function(params){
	if (params == null || params == "null" || typeof(params) == "undefined"){
		return "";
	}else{
		return params;
	}
};

StrUtil.IsEmpty = function(params){
	if (params == null || params=="" || params == "null" || typeof(params) == "undefined"){
		return true;
	}else{
		return false;
	}
}

